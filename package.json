{
  "name": "sbook",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "NODE_ENV=development jest --verbose",
    "test:coverage": "NODE_ENV=development jest --verbose --coverage",
    "eject": "react-scripts eject",
    "postinstall": "link-module-alias",
    "storybook": "start-storybook -p 6006",
    "build-sb": "rm -rf lib && babel src --out-dir lib --source-maps inline --copy-files",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@storybook/addon-actions": "^5.2.0",
    "@storybook/addon-backgrounds": "^5.2.0",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-info": "^5.2.0",
    "@storybook/addon-links": "^5.2.0",
    "@storybook/addon-notes": "^5.2.0",
    "@storybook/addon-viewport": "^5.2.0",
    "@storybook/addons": "^5.2.0",
    "@storybook/react": "^5.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "jest": "^24.9.0",
    "link-module-alias": "^1.2.0",
    "prop-types": "^15.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "_moduleAliases": {
    "sbook": "./src"
  },
  "publishConfig": {
    "access": "public"
  }
}
